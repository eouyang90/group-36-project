<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:background="@color/white_background"
    android:layout_height="match_parent">
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scaleType="centerCrop"
        android:adjustViewBounds="true"
        android:id="@+id/history_detail"
        android:src="@drawable/history_detail_background"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />
    <com.jjoe64.graphview.GraphView
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:id="@+id/graph"
        android:layout_marginBottom="64dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />
    <ImageView
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:id="@+id/type_pic"
        android:src="@drawable/adult"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="30dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/date"
        android:textSize="30dp"
        android:textColor="#0F3F24"
        android:text="MM DD, YYYY"
        android:layout_marginTop="18dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="40dp" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/minute"
        android:textSize="20dp"
        android:textColor="#0F3F24"
        android:text="x min y sec"
        android:layout_above="@+id/patient_type"
        android:layout_alignStart="@+id/date" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/patient_type"
        android:textSize="20dp"
        android:textColor="#0F3F24"
        android:text="Patient Type: XXX"
        android:layout_alignBottom="@+id/type_pic"
        android:layout_alignStart="@+id/minute" />
    <TextView
        android:layout_marginLeft="-10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/depth_accuracy"
        android:textSize="35dp"
        android:textColor="#0F3F24"
        android:text="xx %"
        android:layout_alignTop="@+id/frequency_accuracy"
        android:layout_alignStart="@+id/patient_type" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/frequency_accuracy"
        android:textSize="35dp"
        android:textColor="#0F3F24"
        android:text="yy %"
        android:layout_marginTop="25dp"
        android:layout_below="@+id/type_pic"
        android:layout_toStartOf="@+id/type_pic" />
    <TextView
        android:layout_marginBottom="15dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/compressions"
        android:textSize="35dp"
        android:textColor="#0F3F24"
        android:text="xx"
        android:layout_alignBottom="@+id/history_detail"
        android:layout_alignStart="@+id/patient_type" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/breaths"
        android:textSize="35dp"
        android:textColor="#0F3F24"
        android:layout_marginLeft="10dp"
        android:text="yy"
        android:layout_alignTop="@+id/compressions"
        android:layout_toEndOf="@+id/patient_type" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        android:textSize="20dp"
        android:textColor="#0F3F24"
        android:text="Depth Accuracy vs. Reps"
        android:layout_marginBottom="20dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
